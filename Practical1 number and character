#include<stdio.h>
#include<malloc.h>
int arr[9];
void inputData();
void printData();
int main(){
	inputData();
	printData();
}
void inputData(){
	int i;
	for(i=0;i<=8;i++)
	{
		printf("Enter the %dth element:",i);
		scanf("%d",arr[i]);
		
	}
	
}
void printData()
{
	int i;
	for(i=0;i<=8;i++)
{
	printf("%d",arr[i]);
}
	
}
